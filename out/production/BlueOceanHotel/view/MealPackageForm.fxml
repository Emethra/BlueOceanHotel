<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="mealPackageContext" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MealPackageController">
   <children>
      <Label layoutX="202.0" layoutY="22.0" text="Meal Packages" textFill="#156cd7">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <ImageView fx:id="imgBack" fitHeight="44.0" fitWidth="63.0" layoutX="14.0" layoutY="14.0" onMouseClicked="#imgBackMouseClick" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assert/arrow%20left.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="106.0" fitWidth="387.0" layoutX="109.0" layoutY="100.0" opacity="0.29" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assert/blue%20ocean%20name.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="106.0" fitWidth="145.0" layoutX="201.0" layoutY="240.0" opacity="0.29" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assert/my%20logo.png" />
         </image>
      </ImageView>
      <Label fx:id="lblAddMeal" layoutX="221.0" layoutY="90.0" onMouseClicked="#lblAddMealMouseClick" text="Add Meal" textFill="#296775">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="lblModifyMeal" layoutX="212.0" layoutY="166.0" onMouseClicked="#lblModifyMealMouseClick" text="Modify Meal" textFill="#296775">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="lblDeleteMeal" layoutX="214.0" layoutY="230.0" onMouseClicked="#lblDeleteMealMouseClick" text="Delete Meal" textFill="#296775">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
